{
    "name": "smpleader/dtm",
    "type": "project",
    "description": "The project based SPT Framework supply core package.",
    "keywords": [
        "dtm", "sdm", "spt-project"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "joomla/di": "~1.3",
        "smpleader/spt": "^0.8.0",
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^2.15",
        "phpmailer/phpmailer": "^6.6.5"
    },
    "require-dev": { 
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "process-timeout":0
    },
    "autoload": { 
        "psr-4": {
            "DTM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
         
    }
}
